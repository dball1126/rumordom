<div class="container page-content">
  <div class="row">
    <div class="span12">
      <div class="row">
        <% if @users.blank? %>
          <h3>Couldn't find a user with your search criteria</h3>
          <% else %>
          <%= will_paginate %>
            <% @users.each do |user| %>
              <div class="span4 box-container">
                <div class="holder">
    <div class="search_results">
      <h4><%= link_to user.name, user %></h4>
    </div>
    </div>
    </div>
    <% end %>
    <% end %>
  </div>
</div>
</div>
</div>
<%= will_paginate %>